				F.Data.DataView.SetValue("WorkOrder","WorkOrder",V.dataview.WorkOrder!WorkOrder.RowCount--,"Gss_Issue",v.Local.fTemp)
				F.Data.DataTable.SetValue("Post",V.datatable.Post.RowCount--,"Gss_Issue",v.Local.fTemp)
			f.Intrinsic.Control.ElseIf(v.Local.fTemp,<,0)
				f.Intrinsic.Math.Add(V.DataView.WorkOrder!WorkOrder(V.dataview.WorkOrder!WorkOrder.RowCount--).Gss_Issue!FieldValFloat,v.Local.fTemp,v.Local.fTemp)
				F.Intrinsic.Math.Round(v.Local.fTemp,4,v.Local.fTemp)
				F.Data.DataView.SetValue("WorkOrder","WorkOrder",V.dataview.WorkOrder!WorkOrder.RowCount--,"Gss_Issue",v.Local.fTemp)
				F.Data.DataTable.SetValue("Post",V.datatable.Post.RowCount--,"Gss_Issue",v.Local.fTemp)
			f.Intrinsic.Control.EndIf
		
		f.Intrinsic.Control.EndIf		
	
	f.Intrinsic.Control.EndIf
	
	

	
F.Intrinsic.Control.EndIf

