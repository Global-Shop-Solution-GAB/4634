V.Local.fAvgDropArea.Declare(Float)
v.Local.sTotalSheets.Declare(String)
v.Local.fCompare.Set(v.DataTable.Drop(V.Args.rowindex).Drop_Issue!FieldValFloat)

v.Local.fCompareAvg.Set(v.Local.fCompare)
F.Intrinsic.Control.If(V.Screen.Form_Interface!cbo_Lot.Text,<>,"")
	F.Intrinsic.String.Build("Select Length, Width From V_INVENTORY_MST2 Where Part = '{0}'",V.DataView.WorkOrder!WorkOrder(V.DataView.WorkOrder!WorkOrder.RowCount--).Material!FieldValTrim,V.Local.sSql)
	F.ODBC.Connection!Con.OpenLocalRecordsetRO("rst2",V.Local.sSql)
	F.Intrinsic.Control.If(V.ODBC.Con!rst2.EOF,=,False)
		F.Intrinsic.String.Build("NestId = '{0}'",V.Screen.Form_Interface!cmboNNestID.Text,V.Local.sFilter)
		F.Data.DataTable.Compute("Material","Sum(Qty)",V.Local.sFilter,V.Local.fQty)
		F.Unofficial.Script.Eval("([V.ODBC.Con!rst2.FieldValFloat!Length]*[V.ODBC.Con!rst2.FieldValFloat!Width])*1",V.Local.fTotalArea)
	F.Intrinsic.Control.EndIf

	F.ODBC.Con!rst2.Close

	F.Intrinsic.String.Build("NestId = '{0}'",V.Screen.Form_Interface!cmboNNestID.Text,V.Local.sFilter)
	F.Data.DataTable.Compute("Material","Sum(Qty)",V.Local.sFilter,V.Local.fQty)
	F.Data.DataTable.SetValue("Drop",V.Args.RowIndex,"Size",V.Args.Value)
	F.Data.DataView.SetValue("Drops","Drops",V.Args.RowHandle,"Size",V.Args.Value)
	F.Data.DataTable.SetValue("Drop",V.Args.RowIndex,"Material",V.Screen.Form_Interface!txt_Material.Text)
	F.Data.DataView.SetValue("Drops","Drops",V.Args.RowHandle,"Material",V.Screen.Form_Interface!txt_Material.Text)
	V.Local.sDrop.Set(V.Args.Value)
	V.Local.sDrop.Set(v.Local.sDrop.UCase)
	F.Intrinsic.Control.If(V.Local.sDrop,=,"")
		V.Local.fDropArea.Set(0)
	F.Intrinsic.Control.Else
		F.Intrinsic.String.Split(V.Local.sDrop.UCase,"X",V.Local.sDrop)

		F.Intrinsic.Math.Mult(V.Local.sDrop(0).Float,V.Local.sDrop(1).Float,V.Local.fDropArea)
			

		F.Intrinsic.Math.Div(V.Local.fDropArea,V.DataTable.Post.RowCount,V.Local.fAvgDropArea)
		
		
		F.Intrinsic.Math.Div(V.Local.fAvgDropArea,V.Local.fTotalArea,V.Local.fAvgDropArea)
		F.Unofficial.Script.Eval("[V.Local.fDropArea]/[V.Local.fTotalArea]",V.Local.fDropPercent)
		F.Intrinsic.Math.Round(V.Local.fDropPercent,4,V.Local.fDropPercent)
		F.Data.DataTable.SetValue("Drop",V.Args.RowIndex,"Drop_Issue",V.Local.fDropPercent)
		F.Data.DataTable.SetValue("Drop",V.Args.RowIndex,"QTY",1)
		F.Data.DataView.SetValue("Drops","Drops",V.Args.RowHandle,"Drop_Issue",V.Local.fDropPercent)
		F.Data.DataView.SetValue("Drops","Drops",V.Args.RowHandle,"QTY",1)
		f.Intrinsic.math.Sub(v.Local.fDropPercent,v.Local.fCompare,v.Local.fCompare)
		f.Intrinsic.Control.if(v.Local.fCompareAvg,<>,0)
			F.Intrinsic.Math.Div(v.Local.fCompare,V.DataTable.Post.RowCount,V.Local.fAvgDropArea)
			f.Intrinsic.Control.If(v.Local.fAvgDropArea,<,0)
				f.Intrinsic.math.Mult(v.Local.fAvgDropArea,-1,v.Local.fAvgDropArea)
			f.Intrinsic.Control.EndIf
		f.Intrinsic.Control.EndIf
		F.Intrinsic.Control.For(V.Local.i,0,V.DataTable.Post.RowCount--,1)
			f.Intrinsic.Control.If(v.Local.fCompare,<,0)
				f.Intrinsic.Math.add(V.DataView.WorkOrder!WorkOrder(V.Local.i).Issue!FieldValFloat,V.Local.fAvgDropArea,V.Local.fGssIssue)
			f.Intrinsic.Control.Else
				f.Intrinsic.Math.Sub(V.DataView.WorkOrder!WorkOrder(V.Local.i).Issue!FieldValFloat,V.Local.fAvgDropArea,V.Local.fGssIssue)
			f.Intrinsic.Control.EndIf
			'F.Intrinsic.Math.Evaluate("([V.DataView.WorkOrder!WorkOrder(V.Local.i).Issue!FieldValFloat]-[V.Local.fAvgDropArea])",V.Local.fGssIssue)
			F.Intrinsic.Math.Round(V.Local.fGssIssue,4,V.Local.fGssIssue)
			F.Data.DataTable.SetValue("Post",V.Local.i,"Issue",V.Local.fGssIssue)
			F.Data.DataView.SetValue("WorkOrder","WorkOrder",V.Local.i,"Issue",V.Local.fGssIssue)
			F.Data.DataTable.SetValue("Post",V.Local.i,"Gss_Issue",V.Local.fGssIssue)
			F.Data.DataView.SetValue("WorkOrder","WorkOrder",V.Local.i,"Gss_Issue",V.Local.fGssIssue)
		F.Intrinsic.Control.Next(v.Local.i)
		
	F.Intrinsic.Control.EndIf

