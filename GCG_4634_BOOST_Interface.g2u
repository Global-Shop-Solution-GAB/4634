	V.Local.iIndex.Set(V.DataView.Drops!Drops(V.Global.iIndex).DataTableIndex)
	
	f.Intrinsic.String.build("Delete From GCG_4634_Boost_DROP Where NestID = '{0}' and ID = {1}",V.DataView.Drops!Drops(V.DataView.Drops!Drops.RowCount--).NestID!FieldValTrim,V.DataView.Drops!Drops(V.Global.iIndex).ID!FieldValLong,v.Local.sSQL)
