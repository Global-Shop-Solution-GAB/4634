	F.Intrinsic.Task.LaunchGSSSync("JB0455","-C",V.Local.sFile)
F.Intrinsic.Control.EndIf

'update all closed flags to closed
F.Intrinsic.String.Build("UPDATE GCG_4634_Boost_MAT SET CLOSED = 'Y' WHERE NESTID = '{0}' ",V.DataView.WorkOrder!WorkOrder(V.DataView.WorkOrder!WorkOrder.RowCount--).NestID!FieldValTrim,V.Local.sSQL)
